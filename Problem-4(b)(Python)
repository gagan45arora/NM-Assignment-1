import time

def fib(n1,n2,num):
	start_time = time.time()
	res=[]
	res.append(n1)
	res.append(n2)
	for i in range (2,num):
		res.append(res[i-1]+res[i-2])
	end_time = time.time()
	print ("time of execution of this function is",(end_time-start_time),"seconds")
	return res[num-1]

num=int(input())
print(fib(0,1,num))
